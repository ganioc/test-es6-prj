{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-console": false,
        "object-literal-sort-keys": false,
        "variable-name": false,
        "max-line-length": false,
        "prefer-for-of": false,
        "ordered-imports": false,
        "quotemark": false,
        "max-classes-per-file": false
    },
    "rulesDirectory": [
        "ts/"
    ]
}